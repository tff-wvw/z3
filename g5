using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp6
{
    internal class Program
    {
        static void Main(string[] args)
        {
            double G, x, z, y, c;

            Console.WriteLine("введите значение переменной y");
            y=Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("введите значение переменной x");
            x=Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("введите значение переменной z");
            z=Convert.ToDouble(Console.ReadLine());

            Console.WriteLine("введите значение переменной c");
            c=Convert.ToDouble(Console.ReadLine());

            G = Math.Tan(Math.Pow(x, 4) - 6) - Math.Cos(Math.Pow(3, (z + x * y))) / Math.Cos(Math.Pow(4, (Math.Pow(x, 3) * Math.Pow(c, 2))));

            Console.WriteLine($"значение переменной G={G}");
            Console.ReadKey();
        }
    }
}
